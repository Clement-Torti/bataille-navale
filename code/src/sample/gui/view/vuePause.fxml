<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #336699;">

    <VBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" style="-fx-background-color: #336700;" minHeight="100" minWidth="100" alignment="CENTER">
        <Button text="Retour" onAction="#resume" textAlignment="LEFT">
            <VBox.margin>
                <Insets bottom="40"/>
            </VBox.margin>
        </Button>
        <Button text="Quitter" onAction="#quit"/>
    </VBox>


</AnchorPane>
